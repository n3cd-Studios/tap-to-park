{
	"name": "ttp-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"frontend": "vite dev --port 8081",
		"backend": "cd api && go run .",
		"start": "concurrently --kill-others \"npm run backend\" \"vite build && vite preview --port 8081\"",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"docs": "cd api && swag init --parseDependency --parseInternal"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.6.0",
		"@fortawesome/free-brands-svg-icons": "^6.6.0",
		"@fortawesome/free-solid-svg-icons": "^6.6.0",
		"@fortawesome/svelte-fontawesome": "^0.2.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/leaflet": "^1.9.12",
		"@vitejs/plugin-basic-ssl": "^1.1.0",
		"autoprefixer": "^10.4.20",
		"concurrently": "^9.0.1",
		"leaflet": "^1.9.4",
		"postcss": "^8.4.47",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.12",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"moment": "^2.30.1",
		"svelte-fa": "^4.0.3",
		"svelte-persisted-store": "^0.11.0"
	}
}
